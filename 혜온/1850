#include<iostream>
#include<cmath>


using namespace std;

int main()
{
	long long a;
	long long b;
	cin >> a >> b;
	long long temp;
	if (b > a)
	{
		temp = b;
		b = a;
		a = temp;
	}
	long long c;
	do
	{
		c = a % b;
		a = b;
		b = c;
		
	} while (b > 0);
	
	for (int i = 0; i <a ; i++)
	{
		cout << 1;
	}
	
}
